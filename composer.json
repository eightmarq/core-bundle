{
    "name": "eightmarq/core-bundle",
    "description": "Core bundle is a basic bundle for all EightMarq bundles.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Norbert Schvoy",
            "email": "norbert.schvoy@eightmarq.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EightMarq\\CoreBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "EightMarq\\CoreBundle\\Tests\\": "tests/" }
    },
    "require": {
        "php": "^8.0",
        "doctrine/common": "^3.0",
        "knplabs/doctrine-behaviors": "^2.0",
        "ramsey/uuid": "^4.0",
        "symfony/framework-bundle": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
